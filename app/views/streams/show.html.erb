<section class="content-header">
  <h1>Stream <small></small></h1>
</section>
<section class="content">
  <div class="row">
    <div class="col-md-4">
    	<div class="box">
    		<div class="box-body">
				<div class="form-group">
				  <strong>Name:</strong>
				  <%= @stream.name %>
				</div>
				 
				<div class="form-group">
				  <strong>RTSP:</strong>
				  <%= @stream.rtsp %>
				</div>
				<%= link_to 'Edit', edit_stream_path(@stream), :class=>'button btn btn-default' %>
				<%= link_to 'Back', streams_path, :class=>'button btn btn-default' %>
			</div>
		</div>
	</div>
  </div>
</section>